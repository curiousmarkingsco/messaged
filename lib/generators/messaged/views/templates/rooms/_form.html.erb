<%= form_with model: room, class: 'messaged-room-form' do |f| %>
  <%= f.label :title, t('messaged.room_name') %>
  <%= f.hidden_field :user_id %>
  <%= f.hidden_field :recipient_id %>
  <%= f.hidden_field :tenant_id %>
  <%= f.text_area :content %>
  <%= f.submit nil, class: 'messaged-button' %>
<% end %>